{
  "expo": {
    "name": "MONK-mode",
    "slug": "MONK-mode",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rudymaze.MONKmode"
    },
    "backgroundColor": "#444444",
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#444444"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/monkmodedarksplash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#444444"
        }
      ],
      "expo-font",
      "expo-build-properties"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0ba9f69a-de0f-4c8d-97b7-a81b4d3bcd5a"
      }
    }
  }
}
